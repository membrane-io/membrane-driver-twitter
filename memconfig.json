{
  "expressions": {
    "tweet": {
      "description": "Tweet",
      "type": "url",
      "regex": "https://twitter.com/[^/]+/status/[0-9]+$",
      "searchIn": "https://twitter.com"
    },
    "user": {
      "description": "User",
      "type": "text",
      "regex": "@\\w{1,15}",
      "searchIn": "https://twitter.com/"
    }
  },
