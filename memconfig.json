{
  "expressions": {
    "tweet": {
      "description": "Tweet",
      "type": "url",
      "regex": "https://twitter.com/[^/]+/status/[0-9]+$",
      "searchIn": "https://twitter.com"
    },
    "user": {
      "description": "User",
      "type": "text",
      "regex": "@\\w{1,15}",
      "searchIn": "https://twitter.com/"
    }
  },
  "schema": {
    "types": [
      {
        "name": "Root",
        "description": "The root type for the Twitter driver, providing core functionalities.",
        "actions": [
          {
            "name": "tweet",
            "type": "Json",
            "params": [
              {
                "name": "text",
                "type": "String"
              },
              {
                "name": "poll_options",
                "type": "String",
                "description": "A list of poll options for a Tweet with a poll, must be comma separated.",
                "optional": true
              },
              {
                "name": "poll_duration_minutes",
                "type": "Int",
                "description": "Duration of the poll in minutes for a Tweet with a poll.",
                "optional": true
              }
            ]
          },
          {
            "name": "endpoint",
            "type": "String",
            "hints": {
              "hidden": true
            },
            "params": [
              {
                "name": "method",
                "type": "String"
              },
              {
                "name": "body",
                "type": "String",
                "optional": true
              },
              {
                "name": "path",
                "type": "String"
              },
              {
                "name": "query",
                "type": "String",
                "optional": true
              },
              {
                "name": "headers",
                "type": "String"
              }
            ]
          },
          {
            "name": "configure",
            "type": "Void",
            "params": [
              {
                "name": "clientSecret",
                "type": "String"
              },
              {
                "name": "clientId",
                "type": "String"
              }
            ]
          }
        ],
        "fields": [
          {
            "name": "user",
            "type": "User",
            "params": [
              {
                "name": "id",
                "type": "String",
                "optional": true
              },
              {
                "name": "username",
                "type": "String",
                "optional": true
              }
            ],
            "description": "Posts a new tweet with the specified text and optional poll."
          },
          {
            "name": "status",
            "type": "String",
            "description": "Internal action to interact with the Twitter API endpoint."
          },
          {
            "name": "tweets",
            "type": "TweetCollection",
            "params": [],
            "description": "Configures the Twitter driver with client credentials."
          },
          {
            "name": "parse",
            "type": "List",
            "hints": {
              "hidden": true
            },
            "ofType": {
              "type": "Ref",
              "ofType": "Tweet"
            },
            "params": [
              {
                "name": "name",
                "type": "String"
              },
              {
                "name": "value",
                "type": "String"
              }
            ]
          },
          {
            "name": "tests",
            "type": "Tests"
          }
        ],
        "events": [
          {
            "name": "statusChanged",
            "type": "Void"
          }
        ]
      },
      {
        "name": "TweetCollection",
        "fields": [
          {
            "name": "one",
            "type": "Tweet",
            "params": [
              {
                "name": "id",
                "type": "String",
                "description": "The tweet id"
              }
            ]
          },
          {
            "name": "page",
            "type": "TweetPage",
            "params": [
              {
                "name": "query",
                "type": "String",
                "optional": true
              },
              {
                "name": "pagination_token",
                "type": "String",
                "description": "Returns results with a Tweet ID less than (that is, older than) the specified ID. The ID specified is exclusive and responses will not include it.",
                "optional": true
              }
            ]
          }
        ],
        "description": "Manages collections of tweets, enabling retrieval and pagination."
      },
      {
        "name": "TweetPage",
        "fields": [
          {
            "name": "items",
            "type": "List",
            "ofType": "Tweet"
          },
          {
            "name": "next",
            "type": "Ref",
            "ofType": "TweetPage"
          }
        ],
        "description": "Represents a page within a paginated collection of tweets."
      },
      {
        "name": "FollowersCollection",
        "fields": [
          {
            "name": "page",
            "type": "FollowersPage",
            "params": [
              {
                "name": "pagination_token",
                "type": "String",
                "optional": true
              },
              {
                "name": "max_results",
                "type": "String",
                "optional": true
              },
              {
                "name": "since_id",
                "type": "String",
                "optional": true
              }
            ]
          }
        ],
        "description": "Handles collections and pagination of a user's followers."
      },
      {
        "name": "FollowersPage",
        "fields": [
          {
            "name": "items",
            "type": "List",
            "ofType": "User"
          },
          {
            "name": "next",
            "type": "Ref",
            "ofType": "FollowersPage"
          }
        ],
        "description": "Represents a page within a paginated collection of a user's followers."
      },
      {
        "name": "MentionsCollection",
        "fields": [
          {
            "name": "page",
            "type": "MentionsPage",
            "params": [
              {
                "name": "pagination_token",
                "type": "String",
                "optional": true
              },
              {
                "name": "max_results",
                "type": "String",
                "optional": true
              },
              {
                "name": "since_id",
                "type": "String",
                "optional": true
              }
            ]
          }
        ],
        "description": "Manages collections and pagination of tweets mentioning a user."
      },
      {
        "name": "MentionsPage",
        "fields": [
          {
            "name": "items",
            "type": "List",
            "ofType": "Tweet"
          },
          {
            "name": "next",
            "type": "Ref",
            "ofType": "MentionsPage"
          }
        ],
        "description": "Represents a page within a paginated collection of tweets mentioning a user."
      },
      {
        "name": "LikedCollection",
        "fields": [
          {
            "name": "page",
            "type": "LikedPage",
            "params": [
              {
                "name": "pagination_token",
                "type": "String",
                "optional": true
              },
              {
                "name": "max_results",
                "type": "String",
                "optional": true
              }
            ]
          }
        ],
        "description": "Manages collections and pagination of tweets liked by a user."
      },
      {
        "name": "LikedPage",
        "fields": [
          {
            "name": "items",
            "type": "List",
            "ofType": "Tweet"
          },
          {
            "name": "next",
            "type": "Ref",
            "ofType": "LikedPage"
          }
        ],
        "description": "Represents a page within a paginated collection of tweets liked by a user."
      },
      {
        "name": "LikingCollection",
        "fields": [
          {
            "name": "page",
            "type": "LikingPage",
            "params": [
              {
                "name": "pagination_token",
                "type": "String",
                "optional": true
              },
              {
                "name": "max_results",
                "type": "String",
                "optional": true
              }
            ]
          }
        ],
        "description": "Handles collections and pagination of users who liked a specific tweet."
      },
      {
        "name": "LikingPage",
        "fields": [
          {
            "name": "items",
            "type": "List",
            "ofType": "User"
          },
          {
            "name": "next",
            "type": "Ref",
            "ofType": "LikingPage"
          }
        ],
        "description": "Represents a page within a paginated collection of users who liked a specific tweet."
      },
      {
        "name": "User",
        "fields": [
          {
            "name": "id",
            "type": "String"
          },
          {
            "name": "username",
            "type": "String",
            "hints": {
              "primary": true
            }
          },
          {
            "name": "name",
            "type": "String"
          },
          {
            "name": "tweets",
            "type": "TweetCollection"
          },
          {
            "name": "followers",
            "type": "FollowersCollection"
          },
          {
            "name": "mentions",
            "type": "MentionsCollection"
          },
          {
            "name": "liked",
            "type": "LikedCollection"
          }
        ],
        "description": "Describes a Twitter user and provides access to their tweets, followers, mentions, and liked tweets."
      },
      {
        "name": "Tweet",
        "fields": [
          {
            "name": "id",
            "type": "String",
            "description": "The unique identifier of the tweet."
          },
          {
            "name": "text",
            "type": "String",
            "hints": {
              "primary": true
            },
            "description": "The content of the tweet."
          },
          {
            "name": "likingUsers",
            "type": "LikingCollection",
            "description": "Collection of users who have liked the tweet."
          }
        ],
        "description": "Describes a tweet and provides access to the users who liked it."
      },
      {
        "name": "MaxRedirectsError",
        "description": "Indicates an error when the maximum number of redirects is reached."
      },
      {
        "name": "Tests",
        "actions": [
          {
            "name": "testTweet",
            "type": "Boolean"
          }
        ],
        "description": "Contains tests to verify the functionality of the Twitter driver."
      }
    ]
  },
  "dependencies": {
    "http": "http:",
    "endpoint": "sys-process:endpointUrl"
  }
}