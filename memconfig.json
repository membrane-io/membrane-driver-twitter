              },
              {
                "name": "clientId",
                "type": "String"
              }
            ]
          }
        ],
        "fields": [
          {
            "name": "user",
            "type": "User",
            "params": [
              {
                "name": "id",
                "type": "String",
                "optional": true
              },
              {
                "name": "username",
                "type": "String",
                "optional": true
              }
            ]
          },
          {
            "name": "status",
            "type": "String"
          },
          {
